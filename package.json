{
  "name": "open-camp",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "dev:worker": "wrangler dev src/worker.ts",
    "build": "tsc && vite build",
    "build:worker": "tsc -p tsconfig.worker.json",
    "preview": "vite preview",
    "deploy:pages": "npm run build && wrangler pages deploy dist",
    "deploy:worker": "wrangler deploy src/worker.ts",
    "db:create": "wrangler d1 create open-camp-db",
    "db:migrate": "wrangler d1 execute open-camp-db --file=./migrations/001_init.sql",
    "db:migrate:local": "wrangler d1 execute open-camp-db --local --file=./migrations/001_init.sql",
    "kv:create": "wrangler kv:namespace create KV",
    "kv:setup": "tsx scripts/setup-kv.ts",
    "typecheck": "tsc && tsc -p tsconfig.worker.json"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-router-dom": "^6.20.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231121.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "tsx": "^4.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "wrangler": "^3.17.1"
  }
}
